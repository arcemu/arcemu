<!--Do not edit this file unless you know what you are doing! Instead, use the configuration tool.-->
<LogonConfiguration>
  <LogonDatabase>
    <Hostname>host</Hostname>
    <Username>username</Username>
    <Password>Passwd</Password>
    <Name>database</Name>
    <Port>3306</Port>
  </LogonDatabase>
  <Listen>
    <Host>0.0.0.0</Host>
    <ISHost>0.0.0.0</ISHost>
    <RealmListPort>3724</RealmListPort>
    <ServerPort>8093</ServerPort>
  </Listen>
  <LogLevel>
    <File>0</File>
  </LogLevel>
  <Rates>
    <AccountRefresh>600</AccountRefresh>
  </Rates>
  <LogonServer>
    <RemotePassword>change_me_logon</RemotePassword>
    <AllowedIPs>127.0.0.1/24</AllowedIPs>
    <AllowedModIPs>127.0.0.1/24</AllowedModIPs>
  </LogonServer>
</LogonConfiguration>